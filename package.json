{
  "name": "lightweight-mock-lib",
  "version": "0.1.0",
  "description": "A lightweight library to generate testing data",
  "author": "chenchen",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "http://mq.code.sangfor.org/31622/lightweight-mock-lib"
  },
  "keywords": [
    "mockdata",
    "data",
    "random",
    "library",
    "lib"
  ],
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn clean && tsc",
    "lint": "xo --quiet",
    "precommit": "lint-staged",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "2.0.0",
    "@types/jest": "27.0.2",
    "@types/node": "14.17.19",
    "husky": "7.0.2",
    "jest": "27.2.2",
    "lint-staged": "11.1.2",
    "prettier": "2.4.1",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.5",
    "typescript": "4.4.3",
    "xo": "0.44.0"
  },
  "xo": {
    "extends": [
      "prettier"
    ]
  },
  "lint-staged": {
    "*.js": [
      "yarn lint",
      "yarn test",
      "prettier --semi false --single-quote --trailing-comma --write",
      "git add"
    ]
  },
  "dependencies": {
    "dayjs": "^1.11.7"
  }
}